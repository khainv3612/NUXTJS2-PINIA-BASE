{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "~/*": [
        "./src/*"
      ]
    },
    "allowJs": true
  },
  "lib": [
    "ESNext",
    "DOM",
    "WebWorker"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "**/*.worker.ts"
  ],
  "include": [
    "./src/**/*"
  ],
  "types": [
    "@pinia/nuxt"
  ]
}
